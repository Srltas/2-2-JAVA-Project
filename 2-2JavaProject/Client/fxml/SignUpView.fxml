<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.SignUpViewController">
   <children>
      <ImageView pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@배경.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="132.0" fitWidth="412.0" layoutX="433.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@이름.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtUserID" layoutX="375.0" layoutY="185.0" onAction="#signUp" prefHeight="63.0" prefWidth="447.0" promptText="아이디">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="374.0" layoutY="267.0" onAction="#signUp" prefHeight="63.0" prefWidth="447.0" promptText="비밀번호">
         <font>
            <Font size="32.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="txtPasswordCheck" layoutX="375.0" layoutY="352.0" onAction="#signUp" prefHeight="63.0" prefWidth="447.0" promptText="비밀번호 확인">
         <font>
            <Font size="32.0" />
         </font>
      </PasswordField>
      <TextField fx:id="txtUserName" layoutX="375.0" layoutY="434.0" onAction="#signUp" prefHeight="63.0" prefWidth="447.0" promptText="닉네임">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </TextField>
      <TextField fx:id="txtUserPhonNumber" layoutX="374.0" layoutY="517.0" onAction="#signUp" prefHeight="63.0" prefWidth="447.0" promptText="전화번호">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </TextField>
      <Button fx:id="btnsignUp" layoutX="314.0" layoutY="606.0" mnemonicParsing="false" onAction="#signUp" prefHeight="53.0" prefWidth="283.0" text="회원가입">
         <font>
            <Font size="29.0" />
         </font>
      </Button>
      <Button fx:id="btnBack" layoutX="605.0" layoutY="607.0" mnemonicParsing="false" onAction="#back" prefHeight="53.0" prefWidth="283.0" text="뒤로" textAlignment="CENTER">
         <font>
            <Font name="Microsoft JhengHei UI Bold" size="27.0" />
         </font>
      </Button>
      <Label fx:id="lblIdStatus" layoutX="386.0" layoutY="248.0" textFill="RED" />
      <Label fx:id="lblPwStatus" layoutX="386.0" layoutY="330.0" textFill="RED" />
      <Label fx:id="lblNameStatus" layoutX="386.0" layoutY="497.0" prefHeight="19.0" prefWidth="11.0" textFill="RED" />
      <Label fx:id="lblPhonNumberStatus" layoutX="386.0" layoutY="580.0" textFill="RED" />
      <Label fx:id="lblPwCheck" layoutX="392.0" layoutY="415.0" textFill="RED" />
   </children>
